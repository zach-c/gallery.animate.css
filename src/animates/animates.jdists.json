var fs = require('fs');
var path = require('path');
var result = {};
forEach(function (filename) {
  filename = path.relative('../../', filename);
  var key = filename.replace(/.*\/([^\/]+\/[^\/]+)\.css$/, '$1');
  result[key] = String(fs.readFileSync(filename));
});
!#{JSON.stringify(result)}
